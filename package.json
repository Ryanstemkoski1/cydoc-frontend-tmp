{
    "name": "cydoc-app",
    "version": "1.3.0",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "build-semantic": "npm run ignore-semantic-changes && cd src/semantic && gulp build-css build-assets",
        "watch-semantic": "npm run ignore-semantic-changes && cd src/semantic && npm run build-semantic && gulp watch",
        "ignore-semantic-changes": "chmod +x ./ignore-semantic-changes.sh && ./ignore-semantic-changes.sh",
        "prettify": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
        "test": "jest --watchAll=false --verbose --maxWorkers 1",
        "test-ci": "jest --watchAll=false --verbose --silent",
        "lint": "next lint"
    },
    "dependencies": {
        "@aws-sdk/credential-provider-cognito-identity": "^3.569.0",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@material-table/core": "^6.4.1",
        "@mui/material": "^5.15.16",
        "@mui/system": "^5.15.15",
        "@sentry/react": "^7.113.0",
        "@stripe/react-stripe-js": "^2.1.0",
        "@stripe/stripe-js": "^1.53.0",
        "@types/react-input-mask": "^3.0.5",
        "@types/react-numeric-input": "^2.2.7",
        "amazon-cognito-identity-js": "^6.3.12",
        "aws-amplify": "^5.3.4",
        "axios": "~0.28.0",
        "colors": "^1.4.0",
        "crypto": "npm:crypto-browserify",
        "crypto-browserify": "^3.12.0",
        "eslint-config-next": "^14.2.3",
        "formik": "^2.4.6",
        "history": "^1.17.0",
        "lodash": "^4.17.21",
        "next": "^14.2.3",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^7.8.2",
        "prop-types": "^15.8.1",
        "react": "^18.3.0",
        "react-cookie": "^6.1.0",
        "react-dom": "^18.3.0",
        "react-input-mask": "^3.0.0-alpha.2",
        "react-masonry-css": "^1.0.16",
        "react-nestable": "^1.3.0",
        "react-print": "^1.3.1",
        "react-qr-code": "^2.0.12",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.3.4",
        "react-smooth-render": "^1.0.0",
        "react-svg-to-image": "^3.0.0",
        "react-to-print": "^2.15.1",
        "react-toastify": "^10.0.5",
        "react-windowed-select": "^5.2.0",
        "stream": "^0.0.2",
        "tiny-invariant": "^1.3.3",
        "update": "^0.7.4",
        "yup": "^1.4.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)?$": "ts-jest",
            "^.+\\.(js|jsx)$": "babel-jest"
        },
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "coveragePathIgnorePatterns": [
            "src/semantic",
            "src/constants"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@cfaester/enzyme-adapter-react-18": "^0.8.0",
        "@cydoc-ai/types": "^0.3.7",
        "@types/enzyme": "^3.10.18",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.1",
        "@types/node": "^20.12.9",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "@types/react-redux": "^7.1.33",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.6",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^29.7.0",
        "del": "^6.1.1",
        "enzyme-to-json": "^3.6.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-jest": "^28.5.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "gulp": "^3.9.1",
        "gulp-chmod": "^3.0.0",
        "jest": "^29.7.0",
        "jest-enzyme": "^7.1.2",
        "jest-watch-typeahead": "^2.2.2",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "react-redux": "^8.0.5",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.9",
        "redux-mock-store": "^1.5.4",
        "redux-thunk": "^2.3.0",
        "sass": "^1.76.0",
        "semantic-ui": "~2.4.2",
        "semantic-ui-react": "^2.1.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.5",
        "uuid": "^9.0.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "bracketSpacing": true,
        "trailingComma": "es5"
    },
    "overrides": {
        "graceful-fs": "^4.2.10"
    },
    "engines": {
        "node": ">=18.20.0"
    }
}
